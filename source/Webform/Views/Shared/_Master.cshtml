
<!DOCTYPE html>
<html class="loading dark-layout" lang="en" data-textdirection="ltr">
@Html.Partial("_MetaTag")
<body class="vertical-layout vertical-menu-modern navbar-floating footer-static" data-open="click" data-menu="vertical-menu-modern" data-col="">
    @Html.Partial("_Header")
    @Html.Partial("_Menu")
    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
        <div class="content-wrapper container-xxl p-0">
            <div class="content-header row">
                <div class="content-header-left col-md-9 col-12 mb-2">
                    <div class="row breadcrumbs-top">
                        <div class="col-12">
                            <h2 class="content-header-title float-start mb-0">@RenderSection("title", required: false)</h2>
                            <div class="breadcrumb-wrapper">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item active">@RenderSection("breadcrumb", required: false)</li>
                                </ol>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="content-body">@RenderBody()</div>
        </div>
    </div>
    @Html.Partial("_Footer")
    @Html.Partial("_Script")
</body>
</html>
@RenderSection("script", required: false)